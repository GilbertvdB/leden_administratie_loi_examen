                <table>
                    <thead>
                        <td>Name</td>
                        <td>Lastname</td>
                        <td>Age</td>
                        <td>Adres</td>
						<td>Contribution</td>
                    </thead>
                    <tbody>
                        <?php $__currentLoopData = $allLedens; $__env->addLoop($__currentLoopData); foreach($__currentLoopData as $leden): $__env->incrementLoopIndices(); $loop = $__env->getLastLoop(); ?>
                            <tr>
                                <td><?php echo e($leden->name); ?></td>
                                <td class="inner-table"><?php echo e($leden->lastname); ?></td>
                                <td class="inner-table"><?php echo e($leden->age); ?></td>
                                <td class="inner-table"><?php echo e($leden->adres); ?></td>
								<td class="inner-table"><?php echo e($leden->contribution); ?></td>
                            </tr>
                        <?php endforeach; $__env->popLoop(); $loop = $__env->getLastLoop(); ?>
                    </tbody>
                </table>
            </div>
        </div>
